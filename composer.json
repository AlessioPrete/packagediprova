{
    "name": "alessioprete/packagediprova",
    "description": "Primo package di prova",
    "license": "MIT",
    "authors": [
        {
            "name": "Alessio Prete",
            "email": "info@alessioprete.com",
            "homepage": "http://localhost"
        }
    ],
    "homepage": "https://github.com/alessioprete/packagediprova",
    "keywords": ["Laravel", "Packagediprova"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Alessioprete\\Packagediprova\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Alessioprete\\Packagediprova\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alessioprete\\Packagediprova\\PackagediprovaServiceProvider"
            ],
            "aliases": {
                "Packagediprova": "Alessioprete\\Packagediprova\\Facades\\Packagediprova"
            }
        }
    }
}
